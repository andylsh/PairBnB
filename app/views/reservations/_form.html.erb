  <div class="form-group">
      <%= form.label :start_date, "Check In" %>:
      <%= form.date_field :start_date, value: Date.today, min: Date.today %>
  </div>

  <div class="form-group">
      <%= form.label :end_date, "Check Out" %>:
      <%= form.date_field :end_date, value: Date.today + 1, min: Date.today + 1  %>
  </div>

  <div class="form-group">
    <%= form.label :guest_no, "Number of Guest" %>:
    <%= form.number_field :guest_no %>
  </div>





